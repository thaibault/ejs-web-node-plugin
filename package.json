{
  "name": "templatewebnodeplugin",
  "version": "1.0.167",
  "license": "CC-BY-3.0",
  "description": "A static template processor for webNode.",
  "homepage": "https://torben.website/templateWebNodePlugin",
  "documentationWebsite": {
    "name": "templateWebNodePlugin",
    "trackingCode": "UA-40192634-19"
  },
  "keywords": [
    "web",
    "backend",
    "static",
    "template"
  ],
  "files": [
    "index.js"
  ],
  "main": "index.compiled.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/templateWebNodePlugin.git"
  },
  "scripts": {
    "build": "webOptimizer build",
    "build:dll": "webOptimizer build:dll",
    "clear": "webOptimizer clear && rimraf dummyPlugin/dummy.txt",
    "document": "webOptimizer document",
    "lint": "webOptimizer check:type; webOptimizer lint",
    "prepare": "yarn build",
    "report:coverage": "nyc report --reporter=text-lcov | coveralls",
    "serve": "yarn build && webNode",
    "start": "yarn serve",
    "test": "nyc yarn test:plain",
    "test:plain": "webOptimizer test",
    "update": "yarn version --patch && yarn publish",
    "update:documentation": "documentationWebsite --log-level info",
    "watch": "webOptimizer build --watch",
    "watch:dll": "webOptimizer build:dll --watch"
  },
  "dependencies": {
    "clientnode": "*",
    "ejs": "*"
  },
  "peerDependencies": {
    "@babel/runtime": "*",
    "web-node": "*"
  },
  "devDependencies": {
    "babel-eslint": "*",
    "coveralls": "*",
    "documentation-website": "*",
    "eslint": "*",
    "eslint-config-google": "*",
    "eslint-plugin-flowtype": "*",
    "eslint-plugin-jsdoc": "*",
    "flow-bin": "*",
    "jsdoc": "*",
    "nyc": "*",
    "qunit": "*",
    "rimraf": "*",
    "web-node": "*",
    "weboptimizer": "*",
    "webpack": "*",
    "webpack-cli": "*",
    "webpack-dev-server": "*"
  },
  "webNode": {
    "template": {
      "cache": false,
      "cacheInPlaceReplacements": false,
      "extensions": [
        ".html.js",
        ".ejs"
      ],
      "inPlaceReplacementPaths": [],
      "locationsToIgnore": [
        "backup",
        "database",
        "log",
        "mockup",
        "node_modules"
      ],
      "options": {
        "cache": true,
        "debug": {
          "__evaluate__": "self.debug"
        },
        "compileDebug": {
          "__evaluate__": "self.debug"
        }
      },
      "renderAfterConfigurationUpdates": false,
      "reloadEntryFiles": true,
      "reloadSourceContent": true,
      "scope": {
        "evaluation": {
          "configuration": "configuration",
          "fileSystem": "fileSystem",
          "now": "now",
          "nowUTCTimestamp": "nowUTCTimestamp",
          "parser": "parser",
          "path": "path",
          "PluginAPI": "PluginAPI",
          "plugins": "plugins",
          "Tools": "Tools"
        },
        "execution": {},
        "plain": {}
      }
    }
  },
  "webOptimizer": {
    "files": {
      "compose": {
        "cascadingStyleSheet": null
      }
    },
    "module": {
      "aliases": {
        "#": "Use the pre-compiled artefacts.",
        "clientnode/test$": "clientnode/test.compiled",
        "web-node/configurator$": "web-node/configurator.compiled",
        "web-node/pluginAPI$": "web-node/pluginAPI.compiled"
      }
    },
    "targetTechnology": "node"
  }
}

{
  "name": "templatewebnodeplugin",
  "version": "1.0.85",
  "license": "CC-BY-3.0",
  "description": "A static template processor for webNode.",
  "homepage": "http://torben.website/templateWebNodePlugin",
  "documentationWebsite": {
    "name": "templateWebNodePlugin",
    "trackingCode": "UA-40192634-19"
  },
  "keywords": [
    "web",
    "backend",
    "static",
    "template"
  ],
  "files": [
    "index.js"
  ],
  "main": "index.compiled.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/templateWebNodePlugin.git"
  },
  "scripts": {
    "build": "webOptimizer build",
    "build:dll": "webOptimizer build:dll",
    "clear": "webOptimizer clear && rimraf dummyPlugin/dummy.txt",
    "document": "webOptimizer document",
    "lint": "webOptimizer check:type; webOptimizer lint",
    "prepare": "npm run build",
    "serve": "npm run build && webNode",
    "start": "npm run serve",
    "test": "webOptimizer test",
    "update": "npm version patch && npm publish",
    "update:documentation": "documentationWebsite --log-level info",
    "watch": "webOptimizer build --watch",
    "watch:dll": "webOptimizer build:dll --watch"
  },
  "dependencies": {
    "clientnode": "^1.0.309",
    "ejs": "^2.5.6"
  },
  "peerDependencies": {
    "babel-runtime": "*",
    "web-node": "*"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "documentation-website": "^1.0.82",
    "eslint": "^4.13.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-jsdoc": "^3.3.0",
    "flow-bin": "^0.61.0",
    "jsdoc": "^3.5.2",
    "qunit": "^2.4.1",
    "rimraf": "^2.6.1",
    "source-map-support": "^0.5.0",
    "web-node": "^1.0.136",
    "weboptimizer": "^1.0.446",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "webNode": {
    "template": {
      "cache": false,
      "cacheInPlaceReplacements": false,
      "extensions": [
        ".html.js",
        ".ejs"
      ],
      "inPlaceReplacementPaths": [],
      "locationsToIgnore": [
        "backup",
        "database",
        "log",
        "mockup",
        "node_modules"
      ],
      "options": {
        "cache": true,
        "debug": {
          "__evaluate__": "self.debug"
        },
        "compileDebug": {
          "__evaluate__": "self.debug"
        }
      },
      "renderAfterConfigurationUpdates": false,
      "reloadEntryFiles": true,
      "reloadSourceContent": true,
      "scope": {
        "evaluation": {
          "configuration": "configuration",
          "fileSystem": "fileSystem",
          "parser": "parser",
          "path": "path",
          "PluginAPI": "PluginAPI",
          "plugins": "plugins",
          "Tools": "Tools"
        },
        "execution": {},
        "plain": {}
      }
    }
  },
  "webOptimizer": {
    "module": {
      "aliases": {
        "clientnode/test$": "clientnode/test.compiled",
        "web-node/configurator$": "web-node/configurator.compiled",
        "web-node/pluginAPI$": "web-node/pluginAPI.compiled"
      },
      "optimizer": {
        "babelMinify": {
          "module": {
            "mangle": false
          }
        }
      }
    }
  }
}

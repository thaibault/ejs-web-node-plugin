{
  "name": "ejs-web-node-plugin",
  "version": "1.0.384",
  "description": "A static template processor for web-node.",
  "keywords": [
    "backend",
    "ejs",
    "static",
    "template"
  ],
  "homepage": "https://torben.website/ejs-web-node-plugin",
  "bugs": {
    "email": "info@torben.website",
    "url": "https://github.com/thaibault/ejs-web-node-plugin/issues"
  },
  "license": "CC-BY-3.0",
  "author": {
    "name": "Torben Sickert",
    "email": "info@torben.website",
    "url": "https://torben.website"
  },
  "files": [
    "index.d.ts",
    "index.js",
    "type.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/ejs-web-node-plugin.git"
  },
  "scripts": {
    "build": "weboptimizer build:types && weboptimizer build",
    "check": "yarn check:types; yarn lint",
    "check:types": "weboptimizer check:types",
    "clear": "weboptimizer clear && rimraf dummyPlugin/dummy.txt",
    "document": "weboptimizer document",
    "lint": "weboptimizer lint",
    "prepare": "yarn build",
    "serve": "yarn build && yarn start",
    "start": "web-node '{plugin: {hotReloading: true}}'",
    "test": "yarn clear && weboptimizer test",
    "test:coverage": "yarn clear && weboptimizer test:coverage",
    "test:coverage:report": "yarn clear && weboptimizer test:coverage:report",
    "update": "yarn version patch && yarn npm publish",
    "update:documentation": "documentation-website",
    "watch": "weboptimizer build --watch"
  },
  "dependencies": {
    "ejs": "^3.1.10"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.9",
    "@babel/runtime": "^7.26.0",
    "@stylistic/eslint-plugin-ts": "^2.11.0",
    "@types/ejs": "^3.1.5",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "clientnode": "^3.0.1229",
    "documentation-website": "^1.0.338",
    "eslint": "^9.15.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jsdoc": "^50.5.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.4",
    "rimraf": "^6.0.1",
    "typescript-eslint": "^8.15.0",
    "web-node": "^1.0.533",
    "weboptimizer": "^2.0.1538"
  },
  "peerDependencies": {
    "@babel/runtime": "*",
    "clientnode": "*",
    "web-node": "*"
  },
  "peerDependenciesMeta": {
    "@babel/runtime": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10",
    "yarn": ">=4"
  },
  "resolutions": {
    "colors": "1.4.0",
    "globals@npm:^14.0.0": "patch:globals@npm%3A11.12.0#~/.yarn/patches/globals-npm-11.12.0-1fa7f41a6c.patch",
    "globals@npm:^11.1.0": "patch:globals@npm%3A11.12.0#~/.yarn/patches/globals-npm-11.12.0-1fa7f41a6c.patch",
    "globals@npm:^9.18.0": "patch:globals@npm%3A11.12.0#~/.yarn/patches/globals-npm-11.12.0-1fa7f41a6c.patch"
  },
  "sideEffects": true,
  "documentationWebsite": {
    "name": "ejs-web-node-plugin",
    "trackingCode": "UA-40192634-19"
  },
  "webNode": {
    "ejs": {
      "cache": false,
      "cacheInPlaceReplacements": false,
      "extensions": [
        ".html.js",
        ".ejs"
      ],
      "locations": {
        "exclude": [
          "backup",
          "database",
          "log",
          "mockup",
          "node_modules",
          "src",
          "source"
        ],
        "include": [],
        "inPlaceReplacements": []
      },
      "options": {
        "cache": true,
        "compileDebug": {
          "__evaluate__": "self.debug"
        },
        "debug": {
          "__evaluate__": "self.debug"
        },
        "localsName": "scope",
        "strict": true,
        "_with": false
      },
      "renderAfterConfigurationUpdates": false,
      "reloadEntryFiles": true,
      "reloadSourceContent": true,
      "scope": {
        "evaluation": {
          "configuration": "configuration",
          "fileSystem": "fileSystem",
          "now": "now",
          "nowUTCTimestamp": "nowUTCTimestamp",
          "parser": "parser",
          "path": "path",
          "pluginAPI": "pluginAPI",
          "plugins": "plugins",
          "synchronousFileSystem": "synchronousFileSystem",
          "array": "array",
          "datetime": "datetime",
          "filesystem": "filesystem",
          "functions": "functions",
          "indicators": "indicators",
          "number": "number",
          "object": "object",
          "require": "require",
          "string": "string",
          "utility": "utility"
        },
        "execution": {},
        "plain": {}
      }
    }
  },
  "webOptimizer": {
    "module": {
      "optimizer": {
        "babelMinify": {
          "module": {
            "#": "Avoids \"TypeError: ./index.ts: Cannot read property 'length' of undefined\"",
            "numericLiterals": false
          }
        }
      }
    },
    "targetTechnology": {
      "boilerplate": "node",
      "payload": "node"
    }
  },
  "packageManager": "yarn@4.5.1+sha512.341db9396b6e289fecc30cd7ab3af65060e05ebff4b3b47547b278b9e67b08f485ecd8c79006b405446262142c7a38154445ef7f17c1d5d1de7d90bf9ce7054d"
}

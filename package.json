{
  "name": "templatewebnodeplugin",
  "version": "1.0.38",
  "license": "CC-BY-3.0",
  "description": "A static template processor for webNode.",
  "homepage": "http://torben.website/templateWebNodePlugin",
  "documentationWebsite": {
    "name": "templateWebNodePlugin",
    "trackingCode": "UA-40192634-19"
  },
  "keywords": [
    "web",
    "backend",
    "static",
    "template"
  ],
  "files": [
    "index.js"
  ],
  "main": "index.compiled.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/templateWebNodePlugin.git"
  },
  "scripts": {
    "build": "webOptimizer build",
    "buildDLL": "webOptimizer buildDLL",
    "clear": "webOptimizer clear && rimraf dummyPlugin/dummy.txt",
    "document": "webOptimizer document",
    "lint": "webOptimizer typeCheck && webOptimizer lint",
    "prepublish": "npm run build",
    "serve": "npm run build && webNode",
    "start": "npm run serve",
    "test": "webOptimizer test",
    "update": "npm version patch && npm publish",
    "updateDocumentation": "documentationWebsite --log-level info",
    "watch": "webOptimizer build --watch",
    "watchDLL": "webOptimizer buildDLL --watch"
  },
  "dependencies": {
    "clientnode": "latest",
    "ejs": "latest",
    "weboptimizer": "latest",
    "web-node": "latest"
  },
  "devDependencies": {
    "documentation-website": "latest",
    "rimraf": "latest",
    "source-map-support": "latest"
  },
  "webNode": {
    "template": {
      "extensions": [
        ".tpl"
      ],
      "locationsToIgnore": [],
      "options": {
        "cache": true,
        "debug": {
          "__evaluate__": "self.debug"
        },
        "compileDebug": {
          "__evaluate__": "self.debug"
        }
      },
      "renderAfterConfigurationUpdates": false,
      "scope": {
        "evaluation": {
          "configuration": "configuration",
          "fileSystem": "fileSystem",
          "parser": "parser",
          "path": "path",
          "pluginAPI": "pluginAPI",
          "plugins": "plugins",
          "tools": "tools"
        },
        "execution": {},
        "plain": {}
      }
    }
  },
  "webOptimizer": {
    "module": {
      "aliases": {
        "clientnode/test$": "clientnode/test.compiled",
        "web-node/configurator$": "web-node/configurator.compiled",
        "web-node/pluginAPI$": "web-node/pluginAPI.compiled"
      },
      "optimizer": {
        "babili": null
      },
      "preprocessor": {
        "javaScript": {
          "options": {
            "presets": {
              "__evaluate__": "(self.targetTechnology === 'node' ? ['es2017-node7'] : [['modern-browsers', {loose: true}], 'es2017']).concat('stage-0')"
            }
          }
        }
      }
    }
  }
}

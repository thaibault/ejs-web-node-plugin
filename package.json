{
  "name": "ejs-web-node-plugin",
  "version": "1.0.344",
  "description": "A static template processor for web-node.",
  "keywords": [
    "backend",
    "ejs",
    "static",
    "template"
  ],
  "homepage": "https://torben.website/ejs-web-node-plugin",
  "bugs": {
    "email": "info@torben.website",
    "url": "https://github.com/thaibault/ejs-web-node-plugin/issues"
  },
  "license": "CC-BY-3.0",
  "author": {
    "name": "Torben Sickert",
    "email": "info@torben.website",
    "url": "https://torben.website"
  },
  "files": [
    "index.d.ts",
    "index.js",
    "type.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/ejs-web-node-plugin.git"
  },
  "scripts": {
    "build": "weboptimizer build:types && weboptimizer build",
    "check": "yarn check:types; yarn lint",
    "check:types": "weboptimizer check:types",
    "clear": "weboptimizer clear && rimraf dummyPlugin/dummy.txt",
    "document": "weboptimizer document",
    "lint": "weboptimizer lint",
    "postversion": "yarn publish --non-interactive; git push --tags && git push",
    "prepare": "yarn build",
    "serve": "yarn build && yarn start",
    "start": "web-node '{plugin: {hotReloading: true}}'",
    "test": "yarn clear && weboptimizer test",
    "test:coverage": "yarn clear && weboptimizer test:coverage",
    "test:coverage:report": "yarn clear && weboptimizer test:coverage:report",
    "update": "yarn version --patch",
    "update:documentation": "documentation-website",
    "watch": "weboptimizer build --watch"
  },
  "dependencies": {
    "ejs": "*"
  },
  "devDependencies": {
    "@babel/eslint-parser": "*",
    "@types/ejs": "*",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "clientnode": "*",
    "documentation-website": "*",
    "eslint": "*",
    "eslint-config-google": "*",
    "eslint-plugin-jsdoc": "*",
    "jest": "*",
    "jsdoc": "*",
    "rimraf": "*",
    "web-node": "*",
    "weboptimizer": "*"
  },
  "peerDependencies": {
    "@babel/runtime": "*",
    "clientnode": "*",
    "web-node": "*"
  },
  "peerDependenciesMeta": {
    "@babel/runtime": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=19",
    "npm": ">=8"
  },
  "resolutions": {
    "colors": "1.4.0"
  },
  "sideEffects": true,
  "documentationWebsite": {
    "name": "ejs-web-node-plugin",
    "trackingCode": "UA-40192634-19"
  },
  "webNode": {
    "ejs": {
      "cache": false,
      "cacheInPlaceReplacements": false,
      "extensions": [
        ".html.js",
        ".ejs"
      ],
      "locations": {
        "exclude": [
          "backup",
          "database",
          "log",
          "mockup",
          "node_modules",
          "src",
          "source"
        ],
        "include": [],
        "inPlaceReplacements": []
      },
      "options": {
        "cache": true,
        "compileDebug": {
          "__evaluate__": "self.debug"
        },
        "debug": {
          "__evaluate__": "self.debug"
        },
        "localsName": "scope",
        "strict": true,
        "_with": false
      },
      "renderAfterConfigurationUpdates": false,
      "reloadEntryFiles": true,
      "reloadSourceContent": true,
      "scope": {
        "evaluation": {
          "configuration": "configuration",
          "fileSystem": "fileSystem",
          "now": "now",
          "nowUTCTimestamp": "nowUTCTimestamp",
          "parser": "parser",
          "path": "path",
          "PluginAPI": "PluginAPI",
          "plugins": "plugins",
          "synchronousFileSystem": "synchronousFileSystem",
          "Tools": "Tools"
        },
        "execution": {},
        "plain": {}
      }
    }
  },
  "webOptimizer": {
    "module": {
      "optimizer": {
        "babelMinify": {
          "module": {
            "#": "Avoids \"TypeError: ./index.ts: Cannot read property 'length' of undefined\"",
            "numericLiterals": false
          }
        }
      }
    },
    "targetTechnology": {
      "boilerplate": "node",
      "payload": "node"
    }
  }
}
